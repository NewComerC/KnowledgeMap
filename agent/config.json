{
  "name": "KnowledgeMap Agent",
  "description": "一个专门用于知识图谱构建和管理的智能体",
  "version": "1.0.0",
  "model": "claude-3.7-sonnet",
  "capabilities": [
    "知识图谱构建",
    "实体关系提取",
    "知识推理",
    "文档分析",
    "代码生成"
  ],
  "environment": {
    "snapshot": "POPULATED_FROM_SETTINGS",
    "install": "pip install -r requirements.txt",
    "terminals": [
      {
        "name": "知识图谱服务",
        "command": "python app.py"
      },
      {
        "name": "数据预处理",
        "command": "python scripts/preprocess.py"
      }
    ]
  },
  "tools": [
    {
      "name": "knowledge_extractor",
      "description": "从文本中提取实体和关系",
      "type": "function"
    },
    {
      "name": "graph_builder",
      "description": "构建知识图谱",
      "type": "function"
    },
    {
      "name": "query_engine",
      "description": "查询知识图谱",
      "type": "function"
    }
  ]
}
